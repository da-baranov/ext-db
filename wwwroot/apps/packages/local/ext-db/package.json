{
    "name": "ExtDb",
    "version": "1.0.0.0",
    "sencha": {
        "name": "ext-db",
        "namespace": "ExtDb",
        "type": "code",
        "framework": "ext",
        "creator": "Db",
        "summary": "",
        "detailedDescription": "This package contains reusable code",
        "version": "1.0.0",
        "compatVersion": "1.0.0",
        "format": "1",
        "output": {
            "base": "${package.dir}/build/${package.name}/${toolkit.name}/${build.id}",
            "js": "..",
            "sass": ""
        },
        "local": true,
        "sass": {
            "namespace": "ExtDb",
            "etc": [
                "${package.dir}/sass/etc/all.scss",
                "${package.dir}/${toolkit.name}/sass/etc/all.scss"
            ],
            "var": [
                "${package.dir}/sass/var",
                "${package.dir}/${toolkit.name}/sass/var"
            ],
            "src": [
                "${package.dir}/sass/src",
                "${package.dir}/${toolkit.name}/sass/src"
            ]
        },
        "classpath": [
            "${package.dir}/src",
            "${package.dir}/${toolkit.name}/src"
        ],
        "overrides": [
            "${package.dir}/overrides",
            "${package.dir}/${toolkit.name}/overrides"
        ],
        "slicer": {
            "js": [
                {
                    "path": "${package.dir}/sass/example/custom.js",
                    "isWidgetManifest": true
                }
            ]
        },
        "resource": {
            "paths": [
                "${package.dir}/resources",
                "${package.dir}/${toolkit.name}/resources"
            ]
        },
        "builds": {
			"classic": {
                "toolkit": "classic",
                "theme": "theme-classic"
            }
			/*
            "triton": {
                "toolkit": "classic",
                "theme": "theme-triton"
            },
            "neptune": {
                "toolkit": "classic",
                "theme": "theme-neptune"
            },
            "crisp": {
                "toolkit": "classic",
                "theme": "theme-crisp"
            },
            "modern-neptune": {
                "toolkit": "modern",
                "theme": "theme-neptune"
            }
			*/
			
        }
    }
}